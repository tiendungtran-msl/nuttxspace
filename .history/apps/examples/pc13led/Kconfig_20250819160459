#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_PC13LED
	tristate "PC13 LED example"
	default n
	---help---
		Enable the OP-TEE GP API client example which uses libteec

if EXAMPLES_PC13LED

config EXAMPLES_PC13LED_PROGNAME
	string "Program name"
	default "pc13led"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config EXAMPLES_PC13LED_PRIORITY
	int "OP-TEE GP task priority"
	default 100

config EXAMPLES_PC13LED_STACKSIZE
	int "OP-TEE GP stack size"
	default DEFAULT_TASK_STACKSIZE

endif
